<%= form_for @event do |f| %>
    <p>
      Create a New Event
    </p>
    <p>
      <%=  f.label :name %>
      <%=  f.text_field :name %>
    </p>
    <p>
      <%=  f.label :start_date %>
      <%=  f.text_field :start_date, :id=>"event_start_date" %>
    </p>
    <p>
      <%=  f.label :start_time %>
      <%=  f.text_field :start_time, :id=>"event_start_time" %>
    </p>
    <p>
      <%=  f.label :end_date %>
      <%=  f.text_field :end_date, :id=>"event_end_date" %>
    </p>
    <p>
      <%=  f.label :end_time %>
      <%=  f.text_field :end_time, :id=>"event_end_time" %>
    </p>
    <p>
      <%=  f.label :description %>
      <%=  f.text_area :description, :id=>"event_description" %>
    </p>
    <p><%=  f.submit %></p>
<% end %>